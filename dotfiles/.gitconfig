[user]
	name = PureFunctor
	email = purefunctor@gmail.com
[core]
	editor = nvim
[pull]
	rebase = false
[init]
	defaultbranch = main
[alias]
	gone = ! git fetch -p && git for-each-ref --format '%(refname:short) %(upstream:track)' | awk '$2 == \"[gone]\" {print $1}' | xargs -r git branch -D
[github]
	user = PureFunctor
